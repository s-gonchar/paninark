{
  "name": "luciam91/slim3-skeleton",
  "description": "Skeleton application for Slim3 with Docker-compose set up for development",
  "license": "MIT",
  "type": "project",
  "keywords": ["slim-framework", "slim3", "skeleton", "docker-compose"],
  "autoload": {
    "psr-4": {
      "Application\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "doctrine/orm": "^2.5",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "phpspec/phpspec": "^2.5"
  }
}
